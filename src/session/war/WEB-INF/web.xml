<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Session</servlet-name>
		<servlet-class>com.session.SessionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Session</servlet-name>
		<url-pattern>/session</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FirstPage</servlet-name>
		<servlet-class>com.session.FirstPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FirstPage</servlet-name>
		<url-pattern>/first</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SecondPage</servlet-name>
		<servlet-class>com.session.SecondPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SecondPage</servlet-name>
		<url-pattern>/second</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ThirdPage</servlet-name>
		<servlet-class>com.session.ThirdPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ThirdPage</servlet-name>
		<url-pattern>/third</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
